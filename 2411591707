        local TeleportsSection = Teleports:NewSection("Shops") 
        TeleportsSection:NewButton("Face shop", "Teleport to Face shop.", function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(127.030167, 724.473389, -461.394958)
        end)
        TeleportsSection:NewButton("Clothing shop", "Teleport to Clothing shop.", function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(136.624741, 724.473389, -572.820618)
        end)
        TeleportsSection:NewButton("Secret shop 1", "Teleport to Secret shop.", function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-546.698242, 723.174072, -586.333435)
        end)
        TeleportsSection:NewButton("Secret shop 2", "Teleport to Clothing shop.", function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1057.50427, 724.773804, -1195.32141)
        end)
        local TeleportsSectionBosses = Teleports:NewSection("Demon Teleports")
        for i,DemonTeleports in pairs(game.Workspace.DemonTeleporters:GetChildren()) do
            TeleportsSectionBosses:NewButton(DemonTeleports.Name, "Teleport to this location.", function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = DemonTeleports.CFrame
            end)
        end

        local tospin = {

        }
        local AutoSpin = Misc:NewSection("AutoSpin")
        local autospinTurn = 0
        AutoSpin:NewToggle("Auto Spin", "Toggle auto spin.", function(state)
            if autospinTurn == 0 then
                autospinTurn = 1
                repeat
                    wait(0.01)
                    if table.find(tospin, game.Players.LocalPlayer.Stats.Class.Value) then
                        wait()
                    else
                        if #tospin == 0 then
                            wait()
                        else
                            if not table.find(tospin, game.Players.LocalPlayer.Stats.Class.Value) then
                                game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer()
                            end
                        end
                    end
                until autospinTurn == 0
            else
                autospinTurn = 0
            end
        end)

        local autobuyTurn = 0
        AutoSpin:NewToggle("Auto Buy Spins", "Toggle auto buy spins.", function(state)
            if autobuyTurn == 0 then
                autobuyTurn = 1
                repeat
                    wait(0.5)
                    game:GetService("ReplicatedStorage").RemoteEvent:FireServer("PurchaseSpinAll")
                until autobuyTurn == 0
            else
                autobuyTurn = 0
            end
        end)

        local Classes = {
            "Cyborg",
            "Esper",
            "Alien",
            "Superhuman",
            "Watchdog",
            "Ninja",
            "Angel",
            "Dark Esper",
            "Metal Bat",
            "Phoenix",
            "Beerus",
            "Demon",
            "Arcane Knight",
            "Broly",
            "Thor",
            "Ultrahuman",
            "Hero Hunter",
            "Gravity",
            "Blast",
            "Bing Bong",
            "Cosmic"
        }
        local ClassSection = Misc:NewSection("Classes")
        for i, Class in pairs(Classes) do
            ClassSection:NewToggle(Class, "Toggle to spin for this class.", function(state)
                if table.find(tospin, Class) then
                    local RemoveNum = 0
                    for i,RemoveClass in pairs(tospin) do
                        RemoveNum = RemoveNum + 1
                        if RemoveClass == Class then
                            table.remove(tospin, RemoveNum)
                        end
                    end
                else
                    table.insert(tospin, Class)
                    for i,v in pairs(tospin) do
                        print(tospin)
                    end
                end      
            end)
        end

        local SettingsSection = Settings:NewSection("Settings") 
        SettingsSection:NewKeybind("Toggle UI Keybind", "Click this then press a button to set new keybind.", Enum.KeyCode.P, function()
            Library:ToggleUI()
        end)
        local autokick = 0
        SettingsSection:NewToggle("Autokick when someone is in the server", "Toggle this to kick yourself when someone is in.", function(state)
            if autokick == 0 then
                autokick = 1
                repeat
                    wait(0.01)
                    if (#game.Players:GetChildren() > 1) and (autokick == 1) then
                        game.Players.LocalPlayer:Kick("Someone is in the server")
                    end
                until autokick == 0
            else
                autokick = 0
            end
        end)
        local antiafk = 0
        SettingsSection:NewToggle("AntiAfk", "Toggle this to not get kick by afk.", function(state)
            if antiafk == 0 then
                antiafk = 1
            else
                antiafk = 0
            end
        end)

        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            if antiafk == 1 then
                vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end
        end)
    end
end
