if game.GameId == 2411591707 then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CloverMain/CloverMain/main/Password"))()
    if table.find(Whitelist, game.Players.LocalPlayer.UserId) then
        local color = "BloodTheme"
        local autofarmTurn = 0
        local autoabilitysTurn = 0
        local autoquestTurn = 0
        local autoattackTurn = 0
        local autoformTurn = 0
        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/adabgsdyaevnxijkazgvnfjvncjbxazvknXJBBF/fsho4s7hf4slfshufsf/main/AHD"))()
        local Window = Library.CreateLib("ScriptX | A Hero's Destiny", color)

        local Home = Window:NewTab("Home")
        local Main = Window:NewTab("AutoFarm")
        local Quests = Window:NewTab("Quests")
        local Teleports = Window:NewTab("Teleports")
        local Misc = Window:NewTab("Misc")
        local Settings = Window:NewTab("Settings")

        local HomeSection = Home:NewSection("Home")

        HomeSection:NewLabel("Welcome "..game.Players.LocalPlayer.Name)
        HomeSection:NewLabel("Credits: ScriptXDeveloper#0248")
        HomeSection:NewLabel("A Hero's Destiny Script")

        local Autofarm = Main:NewSection("Autofarm")
        local ToFarm = {

        }
        local function abiltiys(pos)
            local Using = game.Players.LocalPlayer.Stats.Class.Value
            Using = Using:gsub(" ", "")
            for x = 1,4 do
                local v1 = "UpdateMousePosition"
                local v2 = pos
                game:GetService("ReplicatedStorage").RemoteEvent:FireServer(v1, v2)
                local v1 = Using.."Attack"..(x + 1)
                local v2 = pos
                local rem = game:GetService("ReplicatedStorage").RemoteEvent
                rem:FireServer(v1, v2)
            end
        end

        local function quests(SelectedNpc)
            local questnum = 0
            if SelectedNpc == "Criminal" then
                questnum = 1
            end
            if SelectedNpc == "Paradiser" then
                questnum = 2
            end
            if SelectedNpc == "Hammerhead" then
                questnum = 3
            end
            if SelectedNpc == "Crablante" then
                questnum = 4
            end
            if SelectedNpc == "Mosquito" then
                questnum = 5
            end
            if SelectedNpc == "Abnormal" then
                questnum = 6
            end
            if SelectedNpc == "Werewolf" then
                questnum = 7
            end
            if SelectedNpc == "Seafolk" then
                questnum = 8
            end
            if SelectedNpc == "SeaKing" then
                questnum = 9
            end
            if SelectedNpc == "Sonic" then
                questnum = 10
            end
            if SelectedNpc == "VaccineMan" then
                questnum = 11
            end
            if SelectedNpc == "MosquitoGirl" then
                questnum = 12
            end
            if SelectedNpc == "Phoenix" then
                questnum = 13
            end
            if SelectedNpc == "Kabuto" then
                questnum = 14
            end
            if SelectedNpc == "Gouketsu" then
                questnum = 15
            end
            if SelectedNpc == "Boros" then
                questnum = 16
            end
            if SelectedNpc == "Charanko" then
                questnum = 17
            end
            if SelectedNpc == "Psykos" then
                questnum = 18
            end
            if SelectedNpc == "Bahiri" then
                questnum = 19
            end
            if SelectedNpc == "Claire" then
                questnum = 20
            end
            if SelectedNpc == "Fendstrum" then
                questnum = 21
            end
            if SelectedNpc == "Voidlet" then
                questnum = 22
            end
            if SelectedNpc == "Void" then
                questnum = 23
            end
            if SelectedNpc == "VoidCrystal" then
                questnum = 24
            end
            if SelectedNpc == "HumanMonster" then
                questnum = 25
            end
            if SelectedNpc == "GoldenS" then
                questnum = 26
            end
            if SelectedNpc == "PlatinumS" then
                questnum = 27
            end
            if SelectedNpc == "AwakenedHumanMonster" then
                questnum = 28
            end
            if SelectedNpc == "Rock" then
                questnum = 29
            end
            if SelectedNpc == "Auroris" then
                questnum = 30
            end
            if SelectedNpc == "Kayla" then
                questnum = 31
            end
            if SelectedNpc == "CosmicHumanMonster" then
                questnum = 32
            end
            local store = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
            game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.new(0,-7,0)
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("GetQuest",questnum)
            wait(0.5)
            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = store
        end

        local player = game.Players.LocalPlayer
        local UnderPlayer = Instance.new("Part")
        UnderPlayer.Parent = game.workspace
        UnderPlayer.CFrame = CFrame.new(0,0,4000)
        UnderPlayer.Size = Vector3.new(5, 0.01, 5)
        UnderPlayer.Anchored = true
        UnderPlayer.Material = Enum.Material.ForceField
        local currentmobquest = "none"
        Autofarm:NewToggle("AutoFarm", "Toggle autofarm!", function(autofarmtoggle)
            if autofarmTurn == 0 then
                autofarmTurn = 1
                repeat
                    wait(0.01)
                    UnderPlayer.CFrame = CFrame.new(player.Character.HumanoidRootPart.CFrame.X, player.Character.HumanoidRootPart.CFrame.Y, player.Character.HumanoidRootPart.CFrame.Z) * CFrame.new(0, -3.0248,0)
                    for i,Npc in pairs(game:GetService("Workspace").Spawns:GetChildren()) do
                        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(0.150586292, 143.59996, -0.193741798, -1, -2.27380017e-08, 2.75634229e-05, -2.27359411e-08, 1, 7.47385442e-08, -2.75634229e-05, 7.4737919e-08, -1)
                        if table.find(ToFarm, Npc.Name) then
                            if Npc:FindFirstChild(Npc.Name) == nil  then
                                else
                                Npc2 = Npc:FindFirstChild(Npc.Name)
                                if Npc2:FindFirstChild("Humanoid") == nil then
                                else
                                    if Npc2:FindFirstChild("Humanoid").Health > 0 then
                                        if autoquestTurn == 1 then
                                            quests(Npc.Name)
                                            wait(0.1)
                                        end
                                        repeat
                                            wait()
                                            if Npc2:FindFirstChild("Humanoid") == nil and Npc2:FindFirstChild("HumanoidRootPart") == nil then
                                            else
                                                if Npc2:FindFirstChild("Humanoid").Health > 0 then
                                                    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =  Npc2:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 3)
                                                    UnderPlayer.CFrame = CFrame.new(player.Character.HumanoidRootPart.CFrame.X, player.Character.HumanoidRootPart.CFrame.Y, player.Character.HumanoidRootPart.CFrame.Z) * CFrame.new(0, -3.0248,0)
                                                    if autoabilitysTurn == 1 then
                                                        local pos = Npc2:FindFirstChild("HumanoidRootPart").Position
                                                        abiltiys(pos)
                                                    end
                                                    if autoattackTurn == 1 then
                                                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer("Punch","Left")
                                                    end
                                                end
                                            end
                                        until autofarmTurn == 0 or Npc2:FindFirstChild("Humanoid").Health == 0 or (not table.find(ToFarm, Npc.Name))
                                    end
                                end
                            end
                        end
                    end
                until autofarmTurn == 0
                UnderPlayer.CFrame = CFrame.new(0,-1000, 400)
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =  CFrame.new(-57.0648, 725.974, -537.419)
            else
                autofarmTurn = 0
                UnderPlayer.CFrame = CFrame.new(0,-1000, 400)
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =  CFrame.new(-57.0648, 725.974, -537.419)
            end
        end)

        Autofarm:NewToggle("AutoAbilitys", "Toggle auto abilitys!", function(autofarmtoggle)
            if autoabilitysTurn == 0 then
                autoabilitysTurn = 1
            else
                autoabilitysTurn = 0
            end
        end)

        Autofarm:NewToggle("AutoAttack", "Toggle auto attack!", function(autofarmtoggle)
            if autoattackTurn == 0 then
                autoattackTurn = 1
            else
                autoattackTurn = 0
            end
        end)

        Autofarm:NewToggle("AutoQuest", "Toggle auto quest!", function(autofarmtoggle)
            if autoquestTurn == 0 then
                autoquestTurn = 1
                if autofarmTurn == 1 then
                    quests(currentmobquest)
                end
            else
                autoquestTurn = 0
            end
        end)

        Autofarm:NewToggle("AutoForm", "Toggle auto form!", function(autofarmtoggle)
            if autoformTurn == 0 then
                autoformTurn = 1
                repeat 
                    wait(0.01)
                    if game.Players.LocalPlayer.Character:FindFirstChild("Form").value == "" then
                        local Using = game.Players.LocalPlayer.Stats.Class.Value
                        Using = Using:gsub(" ", "")
                        local v1 = Using.."Attack6"
                        local rem = game:GetService("ReplicatedStorage").RemoteEvent
                        rem:FireServer(v1)
                        wait(2)
                    end
                until autoformTurn == 0
            else
                autoformTurn = 0
            end
        end)

        local SelectNpcs = Main:NewSection("SelectNpcs")
        SelectNpcs:NewToggle("Criminal", "Toggle to farm this npc.", function(state)
            if table.find(ToFarm, "Criminal") then
                local RemoveNum = 0
                for i,RemoveNpcName in pairs(ToFarm) do
                    RemoveNum = RemoveNum + 1
                    if RemoveNpcName == "Criminal" then
                        table.remove(ToFarm, RemoveNum)
                    end
                end
            else
                table.insert(ToFarm, "Criminal")
            end
        end)
