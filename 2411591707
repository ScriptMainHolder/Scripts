        local InstaQuest = Quests:NewSection("Take Quests")
        InstaQuest:NewButton("Criminal Quest", "Instant take quest.", function()
            questnum = 1
            local storeCFrame = game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame
            game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.new(0,-7,0)
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("GetQuest",questnum)
            wait(1)
            game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame = storeCFrame
        end)

        InstaQuest:NewButton("Paradiser Quest", "Instant take quest.", function()
            questnum = 2
            local storeCFrame = game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame
            game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.new(0,-7,0)
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("GetQuest",questnum)
            wait(1)
            game.Workspace.StaticHumanoids:FindFirstChild("Quest "..questnum).HumanoidRootPart.CFrame = storeCFrame
        end)

